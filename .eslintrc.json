{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "next",
    "next/core-web-vitals"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "no-extra-semi": "off",
    "react/prop-types": "off",
    "no-unused-vars": "off",
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      2,
      "never"
    ],
    "camelcase": "off"
  }
}